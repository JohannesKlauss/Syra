{
  "canisters": {
    "project": {
      "main": "backend/project/main.mo",
      "type": "motoko"
    },
    "user": {
      "main": "backend/user/main.mo",
      "type": "motoko"
    },
    "syra_assets": {
      "dependencies": [
        "project",
        "user"
      ],
      "frontend": {
        "entrypoint": "public/ic-injector.js"
      },
      "source": [
        "dist/syra_assets/",
        "build"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "output": "canisters/"
    }
  },
  "networks": {
    "local": {
      "bind": "localhost:8000",
      "type": "ephemeral"
    }
  },
  "dfx": "0.6.7",
  "version": 1
}
