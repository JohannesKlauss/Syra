{
  "canisters": {
    "user": {
      "main": "backend/user/main.mo",
      "type": "motoko"
    },
    "syra_frontend": {
      "dependencies": [
        "user"
      ],
      "frontend": {
        "entrypoint": "build/index.html",
        "output": "/"
      },
      "source": [
        "build/audio",
        "build/icons",
        "build/soul",
        "build/static",
        "build/worker",
        "build/worklets",
        "build/_redirects",
        "build/asset-manifest.json",
        "build/index.html",
        "build/favicon.ico",
        "build/logo192.png",
        "build/logo512.png",
        "build/manifest.json",
        "build/precache-manifest.cecab71be1d12a31e9ed1785dae1ae86.js",
        "build/robots.txt",
        "build/service-worker.js"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "output": "canisters/"
    }
  },
  "networks": {
    "local": {
      "bind": "localhost:8000",
      "type": "ephemeral"
    },
    "tungsten": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "dfx": "0.6.6",
  "version": 1
}
