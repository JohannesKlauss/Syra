{
  "name": "splinter.io",
  "version": "0.1.0",
  "repository": "https://JohannesKlauss@github.com/JohannesKlauss/splinter.io",
  "author": "Johannes Klauss",
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@urql/devtools": "2.0.2",
    "graphql": "15.2.0",
    "lodash": "^4.17.15",
    "piano-utils": "^0.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hotkeys-hook": "^2.1.4",
    "react-piano": "^3.1.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "recoil": "0.0.10",
    "tone": "14.7.12",
    "urql": "1.9.8",
    "webmidi": "^2.5.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.16.2",
    "@graphql-codegen/introspection": "1.16.2",
    "@graphql-codegen/typescript": "1.16.2",
    "@graphql-codegen/typescript-operations": "1.16.2",
    "@graphql-codegen/typescript-urql": "1.16.2",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.3",
    "@testing-library/user-event": "12.0.11",
    "@types/jest": "26.0.3",
    "@types/lodash": "^4.14.157",
    "@types/node": "12.12.47",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@welldone-software/why-did-you-render": "4.2.5",
    "prettier": "2.0.5",
    "typescript": "3.9.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gql-gen": "graphql-codegen --config codegen.yml"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "standard"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-native"
    ],
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": false,
      "jasmine": false,
      "beforeAll": false,
      "afterAll": false,
      "beforeEach": false,
      "afterEach": false,
      "test": false,
      "expect": false,
      "describe": false,
      "jest": false,
      "it": false
    },
    "rules": {
      "no-unused-vars": 0,
      "no-undef": 0,
      "react/no-unescaped-entities": 0,
      "react/prop-types": "off",
      "react-native/no-raw-text": 0,
      "space-before-function-paren": 0,
      "@typescript-eslint/ban-ts-ignore": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-object-literal-type-assertion": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
