    "@nestjs/jwt": "7.2.0",
    "@nestjs/microservices": "7.6.5",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-fastify": "7.6.5",
    "@nestjs-modules/mailer": "1.5.1",
    "@prisma/client": "2.11.0",
    "@types/busboy": "0.2.3",
    "@types/passport-local": "1.0.33",
    "apollo-server-fastify": "2.19.1",
    "aws-sdk": "2.827.0",
    "bcrypt": "5.0.0",
    "class-validator": "0.13.1",
    "crypto-js": "4.0.0",
    "fastify-multipart": "3.3.1",
    "fastify-secure-session": "2.3.0",
    "generate-password": "1.5.1",
    "graphql": "15.4.0",
    "graphql-redis-subscriptions": "2.3.1",
    "graphql-subscriptions": "1.1.0",
    "nestjs-redis": "1.2.8",
    "nodemailer": "6.4.17",
    "passport": "0.4.1",
    "passport-cookie": "1.0.9",
    "passport-local": "1.0.0",
    "pug": "3.0.0",
    "stream-chat": "2.11.2",
    "tailwindcss": "2.0.2",
    "type-graphql": "1.1.1",
    "typegraphql-nestjs": "0.2.5",
    "typegraphql-prisma": "0.8.5",
    "uniqid": "5.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.4",
    "@nestjs/schematics": "7.2.6",
    "@nestjs/testing": "7.6.5",
    "@prisma/cli": "2.11.0",
    "@types/bcrypt": "3.0.0",
    "@types/crypto-js": "4.0.1",
    "@types/graphql-fields": "1.3.3",
    "@types/jest": "26.0.20",
    "@types/node": "13.13.39",
    "@types/passport-jwt": "3.0.3",
    "@types/supertest": "2.0.10",
    "@types/uniqid": "5.2.0",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "graphql-fields": "2.0.3",
    "graphql-type-json": "0.3.2",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "reflect-metadata": "0.1.13",
