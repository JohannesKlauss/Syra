{
  "compilerOptions": {
    "target": "esnext",
    "outDir": "./",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "strict": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "removeComments": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "../../node_modules"],
  "include": [
    "generated"
  ]
}