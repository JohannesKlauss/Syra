{
  "name": "@syra/daw",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@inlet/react-pixi": "5.1.5",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@urql/devtools": "2.0.2",
    "array-move": "3.0.1",
    "fm.liveswitch": "1.9.4-33424",
    "graphql": "15.3.0",
    "hotkeys-js": "3.8.1",
    "konva": "7.1.3",
    "lodash": "4.17.20",
    "piano-utils": "0.1.0",
    "pixi.js": "5.3.3",
    "react": "16.13.1",
    "react-color": "2.18.1",
    "react-dom": "16.13.1",
    "react-dropzone": "11.2.0",
    "react-fps": "1.0.2",
    "react-hotkeys-hook": "2.3.1",
    "react-movable": "2.5.1",
    "react-piano": "3.1.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "recoil": "0.0.13",
    "tinycolor2": "1.4.2",
    "tone": "14.7.58",
    "uniqid": "5.2.0",
    "urql": "1.10.1",
    "web-audio-beat-detector": "8.0.30",
    "webmidi": "2.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "standard"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-native"
    ],
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": false,
      "jasmine": false,
      "beforeAll": false,
      "afterAll": false,
      "beforeEach": false,
      "afterEach": false,
      "test": false,
      "expect": false,
      "describe": false,
      "jest": false,
      "it": false
    },
    "rules": {
      "no-unused-vars": 0,
      "no-undef": 0,
      "react/no-unescaped-entities": 0,
      "react/prop-types": "off",
      "react-native/no-raw-text": 0,
      "space-before-function-paren": 0,
      "@typescript-eslint/ban-ts-ignore": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-object-literal-type-assertion": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-var-requires": 0,
      "react": [
        "warn",
        {
          "additionalHooks": "useRecoilCallback"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
